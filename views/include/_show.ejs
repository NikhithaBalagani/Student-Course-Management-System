<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <!--td><%= i + 1 %></td-->
        <td><%= users[i].cno %></td>
        <td><%= users[i].course %></td>
        <td><%= users[i].professor %></td>
        <td><%= users[i].grade %></td>
        <td><%= users[i].status %></td>
        <td>
            <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
                <span class="text-mason"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
                <span class="text-mason"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>